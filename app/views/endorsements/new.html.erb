<%= form_for @endorsement do |f| %>
  <%= f.label :headline %>
  <%= f.text_field :headline %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit "Create endorsement" %>
<% end %>
